<resources>

    <declare-styleable name="SettingBasic">
        <attr name="settingIcon" format="reference" />
        <attr name="settingIconColor" format="color" />
        <attr name="settingIconBackgroundColor" format="color" />
        <attr name="settingTitle" format="reference" />
        <attr name="settingTitleSize" format="float" />
        <attr name="settingTitleColor" format="color" />
        <attr name="settingCaption" format="reference" />
        <attr name="settingCaptionSize" format="float" />
        <attr name="settingCaptionColor" format="color" />
    </declare-styleable>

    <declare-styleable name="SettingWithSwitchView">
        <attr name="settingIcon" />
        <attr name="settingIconColor" />
        <attr name="settingIconBackgroundColor" />
        <attr name="settingTitle" />
        <attr name="settingTitleSize" />
        <attr name="settingTitleColor" />
        <attr name="settingCaption" />
        <attr name="settingCaptionSize" />
        <attr name="settingCaptionColor" />
        <attr name="settingDefaultValue" format="boolean" />
    </declare-styleable>

    <declare-styleable name="FilledImageProgressBar">
        <attr name="ringBackgroundColor" format="color" />
        <attr name="ringBackgroundImage" format="reference" />
        <attr name="ringBackgroundImageDistance" format="float" />
        <attr name="ringBackgroundImageColor" format="color" />
        <attr name="ringColor" format="color" />
        <attr name="ringRadius" format="float" />
        <attr name="ringEdgeWidth" format="float" />
        <attr name="textColor" format="color" />
        <attr name="totalProgress" format="float" />
        <attr name="currentProgress" format="float" />
        <attr name="showImage" format="boolean" />
    </declare-styleable>

</resources>
